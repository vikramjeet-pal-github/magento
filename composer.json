{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "affirm/magento2": "^3.0",
        "aheadworks/module-onestepcheckout": "^1.6",
        "aheadworks/module-rma": "^1.4",
        "aws/aws-sdk-php": "3.*",
        "classyllama/module-avatax": "^1.5",
        "clyde/module-warranty": "^2.0",
        "doctrine/cache": "~1.4",
        "firebase/php-jwt": "^5.0",
        "magento/product-community-edition": "2.3.4-p2",
        "mageplaza/module-smtp": "^1.2",
        "nesbot/carbon": "^2.16",
        "stripe/stripe-php": "^7.33.0",
        "zendesk/zendesk_api_client_php": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.14.0",
        "lusitanian/oauth": "~0.8.10",
        "magento/magento2-functional-testing-framework": "2.4.5",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "~3.4.0"
    },
    "replace": {
        "vertex/product-magento-module": "*",
        "vertex/module-tax": "*",
        "magento/google-shopping-ads": "*",
        "magento/module-dhl": "*",
        "magento/module-marketplace": "*",
        "magento/module-google-adwords": "*",
        "yotpo/magento2-module-yotpo-reviews": "*",
        "dotmailer/dotmailer-magento2-extension": "*",
        "dotmailer/dotmailer-magento2-extension-package": "*"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "version": "2.3.0",
    "minimum-stability": "stable",
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com"
        },
        "aheadworks": {
            "type": "composer",
            "url": "https://dist.aheadworks.com/"
        },
        "clyde": {
            "type": "composer",
            "url": "https://php.fury.io/xumulus/"
        }
    },
    "extra": {
        "magento-force": "override"
    }
}
