<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <body>
      <referenceBlock name="stripe-payments-customer-cards" remove="true"/>
      <referenceBlock name="test-link2" remove="true"/>
      <referenceBlock name="customer_account_navigation">
         <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-subscriptions">
            <arguments>
               <argument name="path" xsi:type="string">subscription/customer/autorefill</argument>
               <argument name="label" xsi:type="string">Auto-refills</argument>
               <argument name="sortOrder" xsi:type="number">240</argument>
            </arguments>
         </block>
         <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-subscription-addresses">
            <arguments>
               <argument name="path" xsi:type="string">subscription/customer/address</argument>
               <argument name="label" xsi:type="string">Address &amp; Payment</argument>
               <argument name="sortOrder" xsi:type="number">150</argument>
            </arguments>
         </block>
      </referenceBlock>
   </body>
</page>