<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
<type name="Magento\Quote\Model\Quote\Item\ToOrderItem">
        <plugin name="product_sku_quote_to_order_item" type="Grazitti\Warranty\Plugin\CustomAttributeQuoteToOrderItem"/>
    </type>   
         <preference for="Grazitti\Warranty\Api\SetAddressInterface" type="Grazitti\Warranty\Model\SetAddress"/> 
         <virtualType name="Magento\Sales\Model\ResourceModel\Order\Grid" type="Magento\Sales\Model\ResourceModel\Grid">
        <arguments>
            <argument name="columns" xsi:type="array">
                <item name="product_sku" xsi:type="string">sales_order.product_sku</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Magento\Sales\Api\OrderRepositoryInterface">
        <plugin name="grazitti_warranty_add_order_extension_attribute" type="Grazitti\Warranty\Plugin\OrderRepositoryPlugin" />         
    </type>
</config>